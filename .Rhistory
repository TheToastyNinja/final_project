left_join(get_mean(five_Bedroom), by = "Year") %>%
left_join(get_mean(Condo_Bedroom), by = "Year") %>%
left_join(get_mean(Duplex_Triplex), by = "Year") %>%
left_join(get_mean(Sfr), by = "Year") %>%
left_join(get_mean(Studio), by = "Year")
